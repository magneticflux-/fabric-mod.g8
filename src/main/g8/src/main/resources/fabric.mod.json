{
  "schemaVersion": 1,
  "id": "$modid$",
  "version": "${version}",
  "name": "$name$",
  "description": "$description$",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/$modid$/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "$package$.ExampleMod"
    ]
  },
  "mixins": [
    "$modid$.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*"
  },
  "suggests": {
    "flamingo": "*"
  }
}
