{
  "schemaVersion": 1,
  "id": "$modid$",
  "version": "\${version}",
  "name": "$name$",
  "description": "$mod_description$",
  "authors": [
    "$author$"
  ],
  "contact": {
    "homepage": "$homepage$",
    "sources": "$sources$"
  },
  "license": "CC0-1.0",
  "icon": "assets/$modid$/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "$package$.$name;format="Camel"$"
    ]
  },
  "mixins": [
    "$modid$.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*"
  },
  "suggests": {
    "flamingo": "*"
  }
}
